{
  "name": "react-components-examples",
  "private": true,
  "version": "5.0.0-alpha13",
  "description": "GoodData ReactComponents examples",
  "main": "index.js",
  "scripts": {
    "examples-heroku-start": "node server.js",
    "examples-build": "NODE_ENV=production webpack",
    "examples-dev-storybook": "webpack-dev-server --env.gdc=http://localhost:9001/ --env.link",
    "examples-dev-secure": "webpack-dev-server --https --env.gdc=https://secure.gooddata.com/ --env.link",
    "examples-dev-staging2": "webpack-dev-server --https --env.gdc=https://staging2.intgdc.com/ --env.link",
    "examples-dev-staging3": "webpack-dev-server --https --env.gdc=https://staging3.intgdc.com/ --env.link",
    "examples-dev-client-demo-be": "webpack-dev-server --https --env.gdc=https://client-demo-be.na.intgdc.com/ --env.link",
    "examples-dev": "yarn dev-secure",
    "examples-stg2": "yarn dev-staging2",
    "examples-stg3": "yarn dev-staging3",
    "examples-validate": "eslint --ext .jsx,.js src test",
    "examples-testcafe": "testcafe chrome --assertion-timeout 5000 -S -s _screenshots test/**/**/*_test*",
    "examples-testcafe-ci": "testcafe chromium --assertion-timeout 5000 -S -s _screenshots test/**/**/*_test*",
    "examples-testcafe-docker": "testcafe 'chrome:headless --no-sandbox' --assertion-timeout 5000 -S -s _screenshots test/**/**/*_test*"
  },
  "keywords": [],
  "author": "GoodData Corporation",
  "license": "BSD-3-Clause"
}
